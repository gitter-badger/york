
	/**
	 * getter for %%name%%

	 * @return %%type%%
	 */
	public function get%%uname%%(){
		return $this->%%name%%;
	}

	/**
	 * setter for %%name%%

	 * @param %%type%% $value
	 * @return \Application\Model\%%model%%
	 */
	public function set%%uname%%($value){
		$this->set('%%name%%', $value);

		return $this;
	}

