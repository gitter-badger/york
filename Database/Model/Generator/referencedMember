	/**
	 * @var %%class%% $%%name%%
	 * @identifiedBy %%identified%%
	*/
	public $%%name%%;

	/**
	 * @deprecated
	 * @not usable!!!
	 */
	private function set%%name%%($%%name%%){
		throw new \York\Exception\Database('setting model items is not supported yet!');
	}
	/**
	 * getter for the %%name%%
	 * @return \Application\Model\%%name%%
	*/
	public function get%%name%%(){
		return $this->%%name%%;
	}


