 * @author York Framework
 * @version 1.0
 * @package Application\Model
 */
class %%modelname%% extends \York\Database\Model\Manager{
	/**
	 * name of the referenced table
	 *
	 * @var string
	 */
	protected $tableName = '%%table%%';

	/**
	 * name of the referenced blueprint class
	 *
	 * @var string
	 */
	protected $blueprint = '\Application\Model\Blueprint\%%modelname%%';

	/**
	 * @param $id
	 * @return \Application\Model\%%modelname%%
	 */
	public function getById($id){
		return parent::getById($id);
	}
}
